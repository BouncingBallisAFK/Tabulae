{
  "scopeName": "source.tadb",
  "fileTypes": [".tadb"],
  "patterns": [
    {
      "name": "keyword.control.tabulae",
      "match": "\\b(maketable|editrow|editcell|query|importcsv|exportcsv|print|runfile|import)\\b"
    },
    {
      "name": "string.quoted.double.tabulae",
      "match": "\"[^\"]*\""
    },
    {
      "name": "constant.numeric.tabulae",
      "match": "\\b\\d+\\b"
    },
    {
      "name": "storage.type.table.tabulae",
      "match": "\\b[A-Za-z_][A-Za-z0-9_]*\\b(?=\\s*\\[)"
    },
    {
      "name": "comment.line.tabulae",
      "match": "#.*$"
    }
  ]
}